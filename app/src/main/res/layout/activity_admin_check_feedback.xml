<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/feedback"
    tools:context=".AdminCheckFeedback">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="115dp"
        android:id="@+id/feedFramId">
        <TextView
            android:layout_width="200dp"
            android:layout_height="40dp"
            android:layout_marginTop="45dp"
            android:layout_marginLeft="115dp"
            android:fontFamily="casual"
            android:textSize="15dp"
            android:textColor="#483d8b"
            android:text="Here You Go With Feedback"/>

    </FrameLayout>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/feedFramId"
        android:id="@+id/feedFramId1">

        <ListView
            android:id="@+id/feedback_List"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="22dp"
            android:layout_marginLeft="15dp"
            android:layout_marginRight="15dp"
            android:layout_marginBottom="45dp"
            android:background="#dcdcdc"/>
    </FrameLayout>

</RelativeLayout>